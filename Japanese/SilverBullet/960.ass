Dialogue: 0,0:00:02.39,0:00:04.98,DefaultSingle-J,,0,0,0,,		おい　いったぞ　灰原
Dialogue: 0,0:00:07.65,0:00:09.53,DefaultSingle-J,,0,0,0,,		おい　何やってんだよ
Dialogue: 0,0:00:09.53,0:00:12.70,DefaultSingle-J,,0,0,0,,		元太君が変なところに蹴るのがいけないんですよ
Dialogue: 0,0:00:12.70,0:00:13.45,DefaultSingle-J,,0,0,0,,		俺
Dialogue: 0,0:00:13.45,0:00:14.20,DefaultSingle-J,,0,0,0,,		はい
Dialogue: 0,0:00:15.03,0:00:17.03,DefaultSingle-J,,0,0,0,,		哀ちゃん　あたし取ってくるね
Dialogue: 0,0:00:17.62,0:00:19.08,DefaultSingle-J,,0,0,0,,		ありがとう　吉田さん
Dialogue: 0,0:00:38.35,0:00:39.18,DefaultSingle-J,,0,0,0,,		吉田さん
Dialogue: 0,0:00:39.64,0:00:40.93,DefaultSingle-J,,0,0,0,,		どうしたんですか
Dialogue: 0,0:00:41.31,0:00:42.23,DefaultSingle-J,,0,0,0,,		あの人
Dialogue: 0,0:00:45.40,0:00:47.36,DefaultSingle-J,,0,0,0,,		何だか寂しそうですね
Dialogue: 0,0:00:47.52,0:00:48.06,DefaultSingle-J,,0,0,0,,		うん
Dialogue: 0,0:00:49.15,0:00:50.86,DefaultSingle-J,,0,0,0,,		おい　誰か来たぞ
Dialogue: 0,0:00:52.74,0:00:53.53,DefaultSingle-J,,0,0,0,,		なるほど　
Dialogue: 0,0:00:53.99,0:00:54.99,DefaultSingle-J,,0,0,0,,		待ち人
Dialogue: 0,0:00:57.45,0:00:58.91,DefaultSingle-J,,0,0,0,,		じゃ　なかったみたいね
Dialogue: 0,0:01:00.12,0:01:01.95,DefaultSingle-J,,0,0,0,,		誰か待ってるんでしょうか
Dialogue: 0,0:01:02.16,0:01:04.08,DefaultSingle-J,,0,0,0,,		んなことどうでもいいだろ
Dialogue: 0,0:01:04.08,0:01:06.00,DefaultSingle-J,,0,0,0,,		いいえ　気になりますよ
Dialogue: 0,0:01:06.21,0:01:09.67,DefaultSingle-J,,0,0,0,,		キレイな女性が１人　公園でお酒を飲んでいる
Dialogue: 0,0:01:10.04,0:01:11.67,DefaultSingle-J,,0,0,0,,		絶対に訳アリですよ
Dialogue: 0,0:01:12.13,0:01:14.13,DefaultSingle-J,,0,0,0,,		何してるか聞いてみようぜ
Dialogue: 0,0:01:14.13,0:01:14.74,DefaultSingle-J,,0,0,0,,		賛成
Dialogue: 0,0:01:14.74,0:01:15.17,DefaultSingle-J,,0,0,0,,		はい
Dialogue: 0,0:01:15.76,0:01:17.22,DefaultSingle-J,,0,0,0,,		おい　オメーら
Dialogue: 0,0:01:17.93,0:01:20.31,DefaultSingle-J,,0,0,0,,		ったく　また余計なことを
Dialogue: 0,0:01:24.02,0:01:24.85,DefaultSingle-J,,0,0,0,,		なあ　なあ　
Dialogue: 0,0:01:26.02,0:01:27.27,DefaultSingle-J,,0,0,0,,		何してんだ
Dialogue: 0,0:01:28.06,0:01:28.98,DefaultSingle-J,,0,0,0,,		えっと　
Dialogue: 0,0:01:30.11,0:01:30.94,DefaultSingle-J,,0,0,0,,		私
Dialogue: 0,0:01:31.36,0:01:34.32,DefaultSingle-J,,0,0,0,,		どうして公園で１人でお酒を飲んでるの
Dialogue: 0,0:01:34.61,0:01:37.78,DefaultSingle-J,,0,0,0,,		あら　ハズかしいところ見られちゃったわね
Dialogue: 0,0:01:38.07,0:01:41.49,DefaultSingle-J,,0,0,0,,		グラスが２つあるのに　お１人なのはどうして
Dialogue: 0,0:01:46.54,0:01:48.75,DefaultSingle-J,,0,0,0,,		ぼ　ぼ　僕は　何も
Dialogue: 0,0:01:48.75,0:01:50.04,DefaultSingle-J,,0,0,0,,		おいおい　泣くなよ
Dialogue: 0,0:01:50.04,0:01:51.42,DefaultSingle-J,,0,0,0,,		おばさん　大丈夫
Dialogue: 0,0:01:52.00,0:01:53.00,DefaultSingle-J,,0,0,0,,		ごめんなさい
Dialogue: 0,0:01:53.80,0:01:55.63,DefaultSingle-J,,0,0,0,,		思い出したら涙が
Dialogue: 0,0:01:55.97,0:01:57.51,DefaultSingle-J,,0,0,0,,		何を思い出したんだ
Dialogue: 0,0:01:58.01,0:01:59.76,DefaultSingle-J,,0,0,0,,		最愛の人のこと
Dialogue: 0,0:02:01.93,0:02:05.73,DefaultSingle-J,,0,0,0,,		彼が　亡くなってしまって
Dialogue: 0,0:02:08.14,0:02:09.10,DefaultSingle-J,,0,0,0,,		おいおい
Dialogue: 0,0:02:09.40,0:02:11.15,DefaultSingle-J,,0,0,0,,		どうするんだよ　これ
Dialogue: 0,0:02:13.02,0:02:15.23,DefaultSingle-J,,0,0,0,,		今　この瞬間　闇に雷光
Dialogue: 0,0:02:15.23,0:02:16.99,DefaultSingle-J,,0,0,0,,		限界超えろ　クールな推理
Dialogue: 0,0:02:17.32,0:02:18.40,DefaultSingle-J,,0,0,0,,		ひとりでお酒　
Dialogue: 0,0:02:18.40,0:02:20.78,DefaultSingle-J,,0,0,0,,		涙のウラにはどんなドラマが待ち受ける
Dialogue: 0,0:02:21.07,0:02:22.58,DefaultSingle-J,,0,0,0,,		たった１つの真実見抜く
Dialogue: 0,0:02:22.58,0:02:25.12,DefaultSingle-J,,0,0,0,,		見た目は子供　頭脳は大人　その名は　
Dialogue: 0,0:02:25.12,0:02:26.54,DefaultSingle-J,,0,0,0,,		名探偵コナン
Dialogue: 0,0:04:20.28,0:04:21.36,DefaultSingle-J,,0,0,0,,		きれ
Dialogue: 0,0:04:25.53,0:04:27.95,DefaultSingle-J,,0,0,0,,		色んなお花がたくさん咲いてるね
Dialogue: 0,0:04:28.16,0:04:29.58,DefaultSingle-J,,0,0,0,,		感激です
Dialogue: 0,0:04:30.37,0:04:31.58,DefaultSingle-J,,0,0,0,,		あっち行ってみよ
Dialogue: 0,0:04:31.75,0:04:32.62,DefaultSingle-J,,0,0,0,,		ハイ
Dialogue: 0,0:04:32.62,0:04:34.62,DefaultSingle-J,,0,0,0,,		おいおい　ちょっと　待てよ
Dialogue: 0,0:04:34.96,0:04:36.38,DefaultSingle-J,,0,0,0,,		お前達
Dialogue: 0,0:04:38.54,0:04:40.55,DefaultSingle-J,,0,0,0,,		みんな元気でいいわね
Dialogue: 0,0:04:41.01,0:04:43.42,DefaultSingle-J,,0,0,0,,		ところで朋子さん　よかったの
Dialogue: 0,0:04:44.30,0:04:46.93,DefaultSingle-J,,0,0,0,,		植物園に連れて来てもらっちゃって
Dialogue: 0,0:04:47.22,0:04:48.22,DefaultSingle-J,,0,0,0,,		いいの　いいの
Dialogue: 0,0:04:48.47,0:04:51.68,DefaultSingle-J,,0,0,0,,		休日を１人で過ごすのも退屈だったし
Dialogue: 0,0:04:52.23,0:04:54.39,DefaultSingle-J,,0,0,0,,		朋子おばさん　こっち来て
Dialogue: 0,0:04:54.64,0:04:56.52,DefaultSingle-J,,0,0,0,,		待ってね　今　行くから
Dialogue: 0,0:04:57.23,0:04:59.65,DefaultSingle-J,,0,0,0,,		でも　これで本当にいいのか
Dialogue: 0,0:04:59.94,0:05:04.82,DefaultSingle-J,,0,0,0,,		まあ　あの子達の相手をしてる間は　悲しんでる暇なんてないでしょうし
Dialogue: 0,0:05:05.57,0:05:07.19,DefaultSingle-J,,0,0,0,,		いい気晴らしなんじゃないの
Dialogue: 0,0:05:07.95,0:05:08.66,DefaultSingle-J,,0,0,0,,		ああ　
Dialogue: 0,0:05:08.66,0:05:12.08,DefaultSingle-J,,0,0,0,,		あの公園で泣かれた時は　どうなるかと思ったけど
Dialogue: 0,0:05:12.58,0:05:14.04,DefaultSingle-J,,0,0,0,,		結果オーライなのかもな
Dialogue: 0,0:05:14.37,0:05:15.08,DefaultSingle-J,,0,0,0,,		そうね
Dialogue: 0,0:05:15.54,0:05:16.71,DefaultSingle-J,,0,0,0,,		大変です
Dialogue: 0,0:05:17.00,0:05:19.21,DefaultSingle-J,,0,0,0,,		歩美ちゃんがどこにもいません
Dialogue: 0,0:05:20.80,0:05:22.51,DefaultSingle-J,,0,0,0,,		迷子のお知らせをいたします
Dialogue: 0,0:05:23.09,0:05:25.76,DefaultSingle-J,,0,0,0,,		赤い洋服を着た吉田歩美ちゃんを
Dialogue: 0,0:05:25.76,0:05:27.59,DefaultSingle-J,,0,0,0,,		お友達が捜しておられます
Dialogue: 0,0:05:28.22,0:05:32.68,DefaultSingle-J,,0,0,0,,		お心当たりのある方は　インフォメーションセンターまでお願いいたします
Dialogue: 0,0:05:36.56,0:05:38.44,DefaultSingle-J,,0,0,0,,		これですぐ見つかりますよ
Dialogue: 0,0:05:38.86,0:05:40.24,DefaultSingle-J,,0,0,0,,		ありがとうございます
Dialogue: 0,0:05:43.23,0:05:46.03,DefaultSingle-J,,0,0,0,,		大丈夫よ　きっと無事だから
Dialogue: 0,0:05:48.32,0:05:48.99,DefaultSingle-J,,0,0,0,,		あの　
Dialogue: 0,0:05:49.37,0:05:52.87,DefaultSingle-J,,0,0,0,,		この子さっきのアナウンスの迷子の子だと思うんだけど
Dialogue: 0,0:05:53.70,0:05:54.45,DefaultSingle-J,,0,0,0,,		歩美ちゃん
Dialogue: 0,0:05:55.71,0:05:57.50,DefaultSingle-J,,0,0,0,,		コナン君　哀ちゃん
Dialogue: 0,0:05:57.71,0:05:58.79,DefaultSingle-J,,0,0,0,,		歩美
Dialogue: 0,0:05:57.71,0:05:58.79,DefaultSingle-J,,0,0,0,,	{\pos(1600,1070)}	歩美ちゃん
Dialogue: 0,0:05:58.79,0:06:01.13,DefaultSingle-J,,0,0,0,,		哀ちゃん　怖かったよ
Dialogue: 0,0:06:01.29,0:06:02.50,DefaultSingle-J,,0,0,0,,	{\pos(600,1070)}	よしよし
Dialogue: 0,0:06:02.84,0:06:03.46,DefaultSingle-J,,0,0,0,,	{\pos(600,1070)}	あの
Dialogue: 0,0:06:05.72,0:06:07.76,DefaultSingle-J,,0,0,0,,		本当にありがとうございます
Dialogue: 0,0:06:08.26,0:06:09.22,DefaultSingle-J,,0,0,0,,		い　いいえ
Dialogue: 0,0:06:09.68,0:06:11.47,DefaultSingle-J,,0,0,0,,		お子さんが無事でなによりです
Dialogue: 0,0:06:12.68,0:06:13.56,DefaultSingle-J,,0,0,0,,		違います
Dialogue: 0,0:06:14.56,0:06:16.35,DefaultSingle-J,,0,0,0,,		この子達はお友達で
Dialogue: 0,0:06:16.52,0:06:18.23,DefaultSingle-J,,0,0,0,,		私に子供はいないんです
Dialogue: 0,0:06:19.35,0:06:20.86,DefaultSingle-J,,0,0,0,,		これは失礼なことを
Dialogue: 0,0:06:21.15,0:06:22.61,DefaultSingle-J,,0,0,0,,		いえ　いいんです
Dialogue: 0,0:06:23.02,0:06:24.94,DefaultSingle-J,,0,0,0,,		子供好きでいらっしゃるんですね
Dialogue: 0,0:06:25.44,0:06:26.11,DefaultSingle-J,,0,0,0,,		ええ
Dialogue: 0,0:06:27.03,0:06:28.20,DefaultSingle-J,,0,0,0,,		自分もです
Dialogue: 0,0:06:32.41,0:06:32.91,DefaultSingle-J,,0,0,0,,		では
Dialogue: 0,0:06:33.41,0:06:33.87,DefaultSingle-J,,0,0,0,,		はい
Dialogue: 0,0:06:36.04,0:06:38.25,DefaultSingle-J,,0,0,0,,		安心したら　ハラ減っちまって
Dialogue: 0,0:06:38.41,0:06:39.50,DefaultSingle-J,,0,0,0,,		ちょうどいいわ
Dialogue: 0,0:06:40.42,0:06:42.38,DefaultSingle-J,,0,0,0,,		お食事ご一緒しませんか
Dialogue: 0,0:06:42.71,0:06:44.30,DefaultSingle-J,,0,0,0,,		お礼もしたいですし
Dialogue: 0,0:06:44.67,0:06:47.42,DefaultSingle-J,,0,0,0,,		いえ　さすがにそれは申し訳ないです
Dialogue: 0,0:06:47.42,0:06:48.93,DefaultSingle-J,,0,0,0,,		ホラ　行きましょ
Dialogue: 0,0:06:48.93,0:06:50.01,DefaultSingle-J,,0,0,0,,		あ　あの
Dialogue: 0,0:06:51.43,0:06:52.30,DefaultSingle-J,,0,0,0,,		いいのか
Dialogue: 0,0:06:52.64,0:06:53.64,DefaultSingle-J,,0,0,0,,		いいんじゃない
Dialogue: 0,0:06:54.01,0:06:56.06,DefaultSingle-J,,0,0,0,,		ホラ　みんなも行くわよ
Dialogue: 0,0:06:56.31,0:06:57.85,DefaultSingle-J,,0,0,0,,		ハーイ
Dialogue: 0,0:06:59.19,0:07:02.77,DefaultSingle-J,,0,0,0,,		梶木浩平さん　会社の社長さんなんですね
Dialogue: 0,0:07:03.15,0:07:06.19,DefaultSingle-J,,0,0,0,,		メンズコスメの製作と販売をしています
Dialogue: 0,0:07:07.90,0:07:09.95,DefaultSingle-J,,0,0,0,,		メンコスズメって何だ
Dialogue: 0,0:07:09.95,0:07:12.41,DefaultSingle-J,,0,0,0,,		メンズコスメですよ　元太君
Dialogue: 0,0:07:12.99,0:07:14.87,DefaultSingle-J,,0,0,0,,		男性化粧品のことだよ　
Dialogue: 0,0:07:15.33,0:07:19.12,DefaultSingle-J,,0,0,0,,		今の時代　男もしっかり身だしなみを整えなきゃな
Dialogue: 0,0:07:19.71,0:07:23.29,DefaultSingle-J,,0,0,0,,		梶木さんって　普段から植物園に行ったりするの
Dialogue: 0,0:07:23.46,0:07:24.09,DefaultSingle-J,,0,0,0,,		ああ　
Dialogue: 0,0:07:24.46,0:07:26.96,DefaultSingle-J,,0,0,0,,		この間　雑誌の取材とかにも答えたけど
Dialogue: 0,0:07:27.34,0:07:30.76,DefaultSingle-J,,0,0,0,,		自然が好きなんで　１人でよく散歩してるんだよ
Dialogue: 0,0:07:31.47,0:07:34.43,DefaultSingle-J,,0,0,0,,		あと　運動も好きで　サッカーなんかもするんだ
Dialogue: 0,0:07:35.05,0:07:36.01,DefaultSingle-J,,0,0,0,,		ホントか　
Dialogue: 0,0:07:36.31,0:07:38.27,DefaultSingle-J,,0,0,0,,		俺達もサッカー得意だぞ
Dialogue: 0,0:07:39.43,0:07:41.14,DefaultSingle-J,,0,0,0,,		じゃあ　今度勝負するかい
Dialogue: 0,0:07:41.60,0:07:43.27,DefaultSingle-J,,0,0,0,,		望むところだぜ
Dialogue: 0,0:07:43.73,0:07:44.65,DefaultSingle-J,,0,0,0,,		ところで
Dialogue: 0,0:07:44.65,0:07:47.44,DefaultSingle-J,,0,0,0,,		梶木さんには彼女とかいるんですか
Dialogue: 0,0:07:48.48,0:07:49.53,DefaultSingle-J,,0,0,0,,		ああ　いや
Dialogue: 0,0:07:49.90,0:07:51.65,DefaultSingle-J,,0,0,0,,		えっと　その
Dialogue: 0,0:07:52.07,0:07:53.91,DefaultSingle-J,,0,0,0,,		私も興味あります
Dialogue: 0,0:07:54.53,0:07:55.57,DefaultSingle-J,,0,0,0,,		どうなんですか
Dialogue: 0,0:07:56.08,0:07:58.58,DefaultSingle-J,,0,0,0,,		お恥ずかしながら　今はいません
Dialogue: 0,0:07:58.74,0:07:59.33,DefaultSingle-J,,0,0,0,,		あら　
Dialogue: 0,0:07:59.62,0:08:02.25,DefaultSingle-J,,0,0,0,,		じゃあ　どんな女性がお好みなんですか
Dialogue: 0,0:08:02.25,0:08:04.38,DefaultSingle-J,,0,0,0,,		いやぁ　まいったな
Dialogue: 0,0:08:09.21,0:08:11.92,DefaultSingle-J,,0,0,0,,		オーイ　おじさん　いったぞ
Dialogue: 0,0:08:13.26,0:08:14.26,DefaultSingle-J,,0,0,0,,		ああ　はぁ
Dialogue: 0,0:08:15.26,0:08:17.81,DefaultSingle-J,,0,0,0,,		オイ　何　ボーっとしてんだよ
Dialogue: 0,0:08:18.35,0:08:19.52,DefaultSingle-J,,0,0,0,,		ああ　ごめん
Dialogue: 0,0:08:22.98,0:08:24.90,DefaultSingle-J,,0,0,0,,		オーイ　いったぞ　オッサン
Dialogue: 0,0:08:25.86,0:08:26.44,DefaultSingle-J,,0,0,0,,		はい
Dialogue: 0,0:08:26.94,0:08:30.23,DefaultSingle-J,,0,0,0,,		なあ　朋子さん前と全然雰囲気違わねぇか
Dialogue: 0,0:08:30.69,0:08:32.61,DefaultSingle-J,,0,0,0,,		彼女なりにせめてるのよ
Dialogue: 0,0:08:34.16,0:08:35.03,DefaultSingle-J,,0,0,0,,		どういう事だよ
Dialogue: 0,0:08:36.62,0:08:39.87,DefaultSingle-J,,0,0,0,,		朋子さんは　梶木さんの好みに合わせてるの
Dialogue: 0,0:08:40.16,0:08:42.12,DefaultSingle-J,,0,0,0,,		だから　なんで
Dialogue: 0,0:08:42.79,0:08:43.29,DefaultSingle-J,,0,0,0,,		バカ
Dialogue: 0,0:08:45.50,0:08:46.79,DefaultSingle-J,,0,0,0,,		スッゲー
Dialogue: 0,0:08:46.79,0:08:47.84,DefaultSingle-J,,0,0,0,,		おいしそう
Dialogue: 0,0:08:47.84,0:08:49.75,DefaultSingle-J,,0,0,0,,		バラエティーに富んでますね
Dialogue: 0,0:08:52.59,0:08:53.58,DefaultSingle-J,,0,0,0,,		うめぇ
Dialogue: 0,0:08:53.97,0:08:55.97,DefaultSingle-J,,0,0,0,,		本当　よかった
Dialogue: 0,0:08:56.59,0:08:58.22,DefaultSingle-J,,0,0,0,,		ほら　汚れてるわよ
Dialogue: 0,0:08:59.26,0:09:00.18,DefaultSingle-J,,0,0,0,,		ありがとう
Dialogue: 0,0:09:02.02,0:09:03.10,DefaultSingle-J,,0,0,0,,		あ　あの　
Dialogue: 0,0:09:03.81,0:09:06.02,DefaultSingle-J,,0,0,0,,		梶木さん　お口に合いましたか
Dialogue: 0,0:09:07.15,0:09:08.90,DefaultSingle-J,,0,0,0,,		ハイ　とても
Dialogue: 0,0:09:09.52,0:09:12.86,DefaultSingle-J,,0,0,0,,		本当ですか　よかったぁ
Dialogue: 0,0:09:15.52,0:09:18.74,DefaultSingle-J,,0,0,0,,		このお弁当に　僕の大好きなミートボールが入っていて
Dialogue: 0,0:09:19.16,0:09:20.28,DefaultSingle-J,,0,0,0,,		もう　感激です
Dialogue: 0,0:09:21.04,0:09:27.67,DefaultSingle-J,,0,0,0,,		こんな食事が毎日できたら　幸せだろうなって思いました
Dialogue: 0,0:09:29.92,0:09:30.80,DefaultSingle-J,,0,0,0,,		えっと
Dialogue: 0,0:09:30.80,0:09:32.51,DefaultSingle-J,,0,0,0,,		それってプロポーズ
Dialogue: 0,0:09:33.34,0:09:35.01,DefaultSingle-J,,0,0,0,,		いっ　いや　そんなつもりは
Dialogue: 0,0:09:35.26,0:09:36.84,DefaultSingle-J,,0,0,0,,		ないというのも違って
Dialogue: 0,0:09:37.68,0:09:39.72,DefaultSingle-J,,0,0,0,,		いや　まいったなぁ
Dialogue: 0,0:09:42.89,0:09:44.56,DefaultSingle-J,,0,0,0,,	{\pos(600,1070)}	今日は何すっかな
Dialogue: 0,0:09:44.89,0:09:46.27,DefaultSingle-J,,0,0,0,,		やっぱサッカーか
Dialogue: 0,0:09:46.44,0:09:48.40,DefaultSingle-J,,0,0,0,,		おうちにお呼ばれしたんですから
Dialogue: 0,0:09:48.40,0:09:49.81,DefaultSingle-J,,0,0,0,,		それは無いでしょう
Dialogue: 0,0:09:49.81,0:09:51.36,DefaultSingle-J,,0,0,0,,		歩美トランプがいい
Dialogue: 0,0:09:51.98,0:09:54.53,DefaultSingle-J,,0,0,0,,		で　なんで博士までついてくるんだよ
Dialogue: 0,0:09:55.15,0:09:57.70,DefaultSingle-J,,0,0,0,,		子供達がお世話になっておるなら
Dialogue: 0,0:09:57.70,0:10:00.20,DefaultSingle-J,,0,0,0,,		お礼を言わんとなと思ってじゃな
Dialogue: 0,0:10:00.37,0:10:03.79,DefaultSingle-J,,0,0,0,,		朋子さんが美人だって言ったから　ついてきたんじゃないの
Dialogue: 0,0:10:03.99,0:10:06.33,DefaultSingle-J,,0,0,0,,		そ　そんなことあるかもね
Dialogue: 0,0:10:10.38,0:10:12.92,DefaultSingle-J,,0,0,0,,		それにしても大豪邸じゃのぉ
Dialogue: 0,0:10:12.92,0:10:15.42,DefaultSingle-J,,0,0,0,,		ここに１人で暮らしておるんかの
Dialogue: 0,0:10:15.63,0:10:16.42,DefaultSingle-J,,0,0,0,,		ああ　
Dialogue: 0,0:10:16.42,0:10:19.39,DefaultSingle-J,,0,0,0,,		旦那さんが亡くなるまでは２人だったんだろうけど
Dialogue: 0,0:10:21.30,0:10:21.85,DefaultSingle-J,,0,0,0,,		はい
Dialogue: 0,0:10:22.64,0:10:23.93,DefaultSingle-J,,0,0,0,,		どちら様ですか
Dialogue: 0,0:10:24.35,0:10:25.93,DefaultSingle-J,,0,0,0,,		えっと　あの
Dialogue: 0,0:10:26.43,0:10:27.85,DefaultSingle-J,,0,0,0,,		阿笠博士だよ
Dialogue: 0,0:10:28.23,0:10:30.15,DefaultSingle-J,,0,0,0,,		まあ　何というか　
Dialogue: 0,0:10:30.15,0:10:32.82,DefaultSingle-J,,0,0,0,,		この子達の保護者のような者でして
Dialogue: 0,0:10:33.07,0:10:34.82,DefaultSingle-J,,0,0,0,,		あら　そうでしたの
Dialogue: 0,0:10:35.19,0:10:37.24,DefaultSingle-J,,0,0,0,,		さあ　どうぞ　お入りください
Dialogue: 0,0:10:37.53,0:10:39.32,DefaultSingle-J,,0,0,0,,		おじゃまします
Dialogue: 0,0:10:40.20,0:10:41.70,DefaultSingle-J,,0,0,0,,		梶木さん　来てるんだ
Dialogue: 0,0:10:41.95,0:10:42.62,DefaultSingle-J,,0,0,0,,		ええ　
Dialogue: 0,0:10:43.03,0:10:46.25,DefaultSingle-J,,0,0,0,,		浩平さんなら　先に上でみんなを待ってるわ
Dialogue: 0,0:10:52.08,0:10:56.46,DefaultSingle-J,,0,0,0,,		今日みんなを呼んだのは　実は特別な報告があって
Dialogue: 0,0:10:56.88,0:10:58.01,DefaultSingle-J,,0,0,0,,		言ったでしょ
Dialogue: 0,0:10:58.38,0:11:00.55,DefaultSingle-J,,0,0,0,,		彼女はもう独り身じゃないって
Dialogue: 0,0:11:02.55,0:11:05.39,DefaultSingle-J,,0,0,0,,		ところで　梶木さんはどこにいるんですか
Dialogue: 0,0:11:05.68,0:11:07.10,DefaultSingle-J,,0,0,0,,		３階のテラスよ
Dialogue: 0,0:11:07.68,0:11:11.40,DefaultSingle-J,,0,0,0,,		さっきまで２人でお食事して　少しお話しして
Dialogue: 0,0:11:12.02,0:11:13.65,DefaultSingle-J,,0,0,0,,		そのあとお酒を飲んだら
Dialogue: 0,0:11:13.94,0:11:15.61,DefaultSingle-J,,0,0,0,,		あの人　眠くなったのね
Dialogue: 0,0:11:16.19,0:11:18.49,DefaultSingle-J,,0,0,0,,		今は少しウトウトしちゃってるわ
Dialogue: 0,0:11:20.36,0:11:21.82,DefaultSingle-J,,0,0,0,,		ケータイ鳴ってますよ
Dialogue: 0,0:11:22.07,0:11:23.20,DefaultSingle-J,,0,0,0,,		俺のじゃねぇぞ
Dialogue: 0,0:11:26.24,0:11:27.70,DefaultSingle-J,,0,0,0,,		私のでもないわ
Dialogue: 0,0:11:29.04,0:11:29.66,DefaultSingle-J,,0,0,0,,		ここよ
Dialogue: 0,0:11:30.21,0:11:31.92,DefaultSingle-J,,0,0,0,,		じゃあ　一体誰の
Dialogue: 0,0:11:41.88,0:11:42.59,DefaultSingle-J,,0,0,0,,		なに
Dialogue: 0,0:11:44.05,0:11:45.18,DefaultSingle-J,,0,0,0,,		い　今のって
Dialogue: 0,0:11:45.18,0:11:46.06,DefaultSingle-J,,0,0,0,,		まさか
Dialogue: 0,0:11:57.82,0:12:00.99,DefaultSingle-J,,0,0,0,,		亡くなったのは梶木浩平さん　４５歳
Dialogue: 0,0:12:01.65,0:12:04.75,DefaultSingle-J,,0,0,0,,		メンズコスメブランドの社長をなさっていたようです
Dialogue: 0,0:12:06.03,0:12:07.24,DefaultSingle-J,,0,0,0,,		それで死因は
Dialogue: 0,0:12:07.66,0:12:10.20,DefaultSingle-J,,0,0,0,,		転落による脳挫傷とのことです
Dialogue: 0,0:12:10.66,0:12:12.79,DefaultSingle-J,,0,0,0,,		どうやら　あそこから
Dialogue: 0,0:12:14.38,0:12:17.17,DefaultSingle-J,,0,0,0,,		それで　あなたが家主の方ですか
Dialogue: 0,0:12:19.13,0:12:19.71,DefaultSingle-J,,0,0,0,,		はい
Dialogue: 0,0:12:20.26,0:12:23.59,DefaultSingle-J,,0,0,0,,		失礼ですが　梶木さんとはどういった関係で
Dialogue: 0,0:12:24.51,0:12:27.39,DefaultSingle-J,,0,0,0,,		彼とは　恋人関係にありました
Dialogue: 0,0:12:27.89,0:12:29.68,DefaultSingle-J,,0,0,0,,		恋人　ですか
Dialogue: 0,0:12:29.97,0:12:30.47,DefaultSingle-J,,0,0,0,,		はい
Dialogue: 0,0:12:31.18,0:12:34.56,DefaultSingle-J,,0,0,0,,		ごく最近知り合って　お付き合いをはじめて
Dialogue: 0,0:12:35.06,0:12:36.78,DefaultSingle-J,,0,0,0,,		今日プロポーズされました
Dialogue: 0,0:12:37.02,0:12:38.15,DefaultSingle-J,,0,0,0,,		今日ですか
Dialogue: 0,0:12:38.48,0:12:41.40,DefaultSingle-J,,0,0,0,,		前から彼には申し出をされていたのですが
Dialogue: 0,0:12:41.99,0:12:45.41,DefaultSingle-J,,0,0,0,,		その　私は主人を亡くしたばかりなので
Dialogue: 0,0:12:46.87,0:12:49.99,DefaultSingle-J,,0,0,0,,		状況から見ても　事件性は低いかと
Dialogue: 0,0:12:50.29,0:12:50.74,DefaultSingle-J,,0,0,0,,		ウム　
Dialogue: 0,0:12:51.20,0:12:53.32,DefaultSingle-J,,0,0,0,,		しかし　まだ断定はできん
Dialogue: 0,0:12:53.46,0:12:54.12,DefaultSingle-J,,0,0,0,,		あのっ
Dialogue: 0,0:12:55.00,0:12:56.29,DefaultSingle-J,,0,0,0,,		これは事故ですよ
Dialogue: 0,0:12:56.71,0:12:58.21,DefaultSingle-J,,0,0,0,,		僕達　見たんですから
Dialogue: 0,0:12:59.25,0:13:00.71,DefaultSingle-J,,0,0,0,,		嘘じゃねぇぞ
Dialogue: 0,0:13:00.71,0:13:02.30,DefaultSingle-J,,0,0,0,,		みんなでテラスに行ったら
Dialogue: 0,0:13:02.46,0:13:04.63,DefaultSingle-J,,0,0,0,,		梶木さんが転んで落ちちゃったの
Dialogue: 0,0:13:13.93,0:13:16.21,DefaultSingle-J,,0,0,0,,		ここから転落したのか
Dialogue: 0,0:13:16.32,0:13:17.81,DefaultSingle-J,,0,0,0,,		そのようですね
Dialogue: 0,0:13:18.11,0:13:20.69,DefaultSingle-J,,0,0,0,,		あなたと梶木さんはここでお酒を
Dialogue: 0,0:13:20.82,0:13:21.32,DefaultSingle-J,,0,0,0,,		はい
Dialogue: 0,0:13:21.73,0:13:23.57,DefaultSingle-J,,0,0,0,,		どのくらい飲まれましたかな
Dialogue: 0,0:13:24.15,0:13:25.52,DefaultSingle-J,,0,0,0,,		私はそんなに
Dialogue: 0,0:13:26.07,0:13:29.78,DefaultSingle-J,,0,0,0,,		でも　彼は酔って軽く眠り込んでしまうくらいには
Dialogue: 0,0:13:30.08,0:13:31.29,DefaultSingle-J,,0,0,0,,		ねえ　これ見てよ
Dialogue: 0,0:13:32.63,0:13:33.54,DefaultSingle-J,,0,0,0,,		ケータイ
Dialogue: 0,0:13:35.00,0:13:36.93,DefaultSingle-J,,0,0,0,,		それはきっと浩平さんのです
Dialogue: 0,0:13:37.50,0:13:40.17,DefaultSingle-J,,0,0,0,,		被害者のスマホがこんなところに
Dialogue: 0,0:13:40.38,0:13:41.80,DefaultSingle-J,,0,0,0,,		僕　わかりました
Dialogue: 0,0:13:42.96,0:13:44.34,DefaultSingle-J,,0,0,0,,		何がわかったの
Dialogue: 0,0:13:44.63,0:13:47.84,DefaultSingle-J,,0,0,0,,		浩平さんが　どうして転落してしまったかですよ
Dialogue: 0,0:13:48.22,0:13:49.35,DefaultSingle-J,,0,0,0,,		マジかよ
Dialogue: 0,0:13:49.35,0:13:49.97,DefaultSingle-J,,0,0,0,,		ええ
Dialogue: 0,0:13:50.30,0:13:52.72,DefaultSingle-J,,0,0,0,,		浩平さんが酔って寝ていたところに
Dialogue: 0,0:13:52.72,0:13:54.22,DefaultSingle-J,,0,0,0,,		電話がかかってきたんです
Dialogue: 0,0:13:56.31,0:13:58.60,DefaultSingle-J,,0,0,0,,		着信音で目を覚ました浩平さんは
Dialogue: 0,0:13:58.81,0:14:00.90,DefaultSingle-J,,0,0,0,,		慌てて電話に出ようとしたんです
Dialogue: 0,0:14:01.40,0:14:05.41,DefaultSingle-J,,0,0,0,,		ですが　手元が滑ってスマホを落っことしてしまい
Dialogue: 0,0:14:06.20,0:14:10.12,DefaultSingle-J,,0,0,0,,		縁のあたりまで転がったスマホを　拾おうと立ち上がったんですが
Dialogue: 0,0:14:10.32,0:14:12.75,DefaultSingle-J,,0,0,0,,		お酒で酔っていたために転倒してしまい
Dialogue: 0,0:14:13.16,0:14:16.12,DefaultSingle-J,,0,0,0,,		その際　掴んだ手すりが腐っていたために
Dialogue: 0,0:14:16.46,0:14:18.92,DefaultSingle-J,,0,0,0,,		運悪く　そのまま外に投げ出されて
Dialogue: 0,0:14:21.92,0:14:24.67,DefaultSingle-J,,0,0,0,,		被害者の血中アルコール濃度からして
Dialogue: 0,0:14:24.92,0:14:27.26,DefaultSingle-J,,0,0,0,,		酩酊状態だったのは　確かです
Dialogue: 0,0:14:28.59,0:14:32.27,DefaultSingle-J,,0,0,0,,		その際　この段差につまずいたとも考えられるな
Dialogue: 0,0:14:34.26,0:14:36.68,DefaultSingle-J,,0,0,0,,		私が彼を１人になんかしたから
Dialogue: 0,0:14:39.31,0:14:41.40,DefaultSingle-J,,0,0,0,,		朋子さんのせいじゃありませんよ
Dialogue: 0,0:14:41.40,0:14:43.19,DefaultSingle-J,,0,0,0,,		そうよ　事故なんだもん
Dialogue: 0,0:14:43.44,0:14:45.65,DefaultSingle-J,,0,0,0,,		ごめんね　ありがとう
Dialogue: 0,0:14:46.69,0:14:47.65,DefaultSingle-J,,0,0,0,,		なんだコレ
Dialogue: 0,0:14:48.57,0:14:50.32,DefaultSingle-J,,0,0,0,,		コンタクトレンズみたいだけど
Dialogue: 0,0:14:51.70,0:14:53.58,DefaultSingle-J,,0,0,0,,		きっと　それは被害者のだよ
Dialogue: 0,0:14:53.87,0:14:57.37,DefaultSingle-J,,0,0,0,,		遺体には　片目しかコンタクトがついていなかったからね
Dialogue: 0,0:14:57.62,0:15:00.33,DefaultSingle-J,,0,0,0,,		目をこすった時に落としちゃったのかな
Dialogue: 0,0:15:00.71,0:15:02.17,DefaultSingle-J,,0,0,0,,		きっと　そうですね
Dialogue: 0,0:15:02.84,0:15:07.76,DefaultSingle-J,,0,0,0,,		特に不審な点もないことだし　事故死の可能性が高そうだな
Dialogue: 0,0:15:09.63,0:15:10.30,DefaultSingle-J,,0,0,0,,		もう　
Dialogue: 0,0:15:10.30,0:15:12.68,DefaultSingle-J,,0,0,0,,		何やってるんですか　元太君
Dialogue: 0,0:15:12.93,0:15:15.55,DefaultSingle-J,,0,0,0,,		わりぃ　わりぃ　ノド渇いちゃってよ
Dialogue: 0,0:15:15.97,0:15:17.60,DefaultSingle-J,,0,0,0,,		気にしなくていいのよ
Dialogue: 0,0:15:22.48,0:15:25.86,DefaultSingle-J,,0,0,0,,		えっと　何か拭く物を取ってくるわね
Dialogue: 0,0:15:36.37,0:15:38.87,DefaultSingle-J,,0,0,0,,		そうか　そういうことか
Dialogue: 0,0:15:40.50,0:15:41.33,DefaultSingle-J,,0,0,0,,		なあ　博士
Dialogue: 0,0:15:42.62,0:15:43.33,DefaultSingle-J,,0,0,0,,		何じゃ
Dialogue: 0,0:15:46.46,0:15:48.55,DefaultSingle-J,,0,0,0,,		では　我々はこれで
Dialogue: 0,0:15:49.13,0:15:50.59,DefaultSingle-J,,0,0,0,,		ちょっと待ってください
Dialogue: 0,0:15:51.97,0:15:53.43,DefaultSingle-J,,0,0,0,,		どうしました　阿笠さん
Dialogue: 0,0:15:53.80,0:15:55.89,DefaultSingle-J,,0,0,0,,		これは事故死ではない
Dialogue: 0,0:15:56.47,0:15:59.77,DefaultSingle-J,,0,0,0,,		梶木さんは誤って転落したのではなく
Dialogue: 0,0:15:59.98,0:16:03.15,DefaultSingle-J,,0,0,0,,		おそらく　そうなるように仕向けられたのじゃ
Dialogue: 0,0:16:03.69,0:16:05.86,DefaultSingle-J,,0,0,0,,		彼の死が他殺だと
Dialogue: 0,0:16:06.06,0:16:06.77,DefaultSingle-J,,0,0,0,,		そうじゃ
Dialogue: 0,0:16:07.52,0:16:09.78,DefaultSingle-J,,0,0,0,,		彼のスマホをよく見てみるのじゃ
Dialogue: 0,0:16:10.19,0:16:13.53,DefaultSingle-J,,0,0,0,,		地面に落としたという割に　傷ひとつないじゃろ
Dialogue: 0,0:16:14.20,0:16:17.24,DefaultSingle-J,,0,0,0,,		確かに　へこみも傷もないようですね
Dialogue: 0,0:16:17.87,0:16:20.66,DefaultSingle-J,,0,0,0,,		手すりの近くにスマホが落ちていたのは
Dialogue: 0,0:16:20.83,0:16:23.08,DefaultSingle-J,,0,0,0,,		梶木さんが落としたからではなく
Dialogue: 0,0:16:23.08,0:16:27.51,DefaultSingle-J,,0,0,0,,		何者かが手すりの近くに置いたからとは考えられんかのぉ
Dialogue: 0,0:16:28.13,0:16:29.38,DefaultSingle-J,,0,0,0,,		何者か
Dialogue: 0,0:16:29.80,0:16:31.51,DefaultSingle-J,,0,0,0,,		それは　一体
Dialogue: 0,0:16:32.26,0:16:36.18,DefaultSingle-J,,0,0,0,,		この家に出入りできる人物は１人しかおらんじゃろ
Dialogue: 0,0:16:37.51,0:16:38.26,DefaultSingle-J,,0,0,0,,		ですが　
Dialogue: 0,0:16:38.43,0:16:41.98,DefaultSingle-J,,0,0,0,,		仮に彼女がスマホを手すりの前に置いたとして
Dialogue: 0,0:16:41.98,0:16:45.78,DefaultSingle-J,,0,0,0,,		それで転落させたというのは　さすがに無理があるのでは
Dialogue: 0,0:16:46.10,0:16:48.90,DefaultSingle-J,,0,0,0,,		もちろん　仕掛けはそれだけではない
Dialogue: 0,0:16:51.24,0:16:56.37,DefaultSingle-J,,0,0,0,,		彼女はある錯覚を利用して　梶木さんを転ばせ転落させたのじゃ
Dialogue: 0,0:16:56.87,0:16:59.70,DefaultSingle-J,,0,0,0,,		錯覚ですか　一体どんな
Dialogue: 0,0:17:00.62,0:17:04.71,DefaultSingle-J,,0,0,0,,		段差の手前でスマホが浮かんで見えるようにしたのじゃよ
Dialogue: 0,0:17:05.00,0:17:06.33,DefaultSingle-J,,0,0,0,,		スマホが
Dialogue: 0,0:17:06.63,0:17:08.79,DefaultSingle-J,,0,0,0,,		そんなことが可能なんですか
Dialogue: 0,0:17:09.63,0:17:12.09,DefaultSingle-J,,0,0,0,,		実際に僕が再現してみせるよ
Dialogue: 0,0:17:12.51,0:17:15.59,DefaultSingle-J,,0,0,0,,		廊下に置いてあった　この霧吹きを使ってね
Dialogue: 0,0:17:15.59,0:17:17.14,DefaultSingle-J,,0,0,0,,		き　霧吹き
Dialogue: 0,0:17:18.64,0:17:21.43,DefaultSingle-J,,0,0,0,,		まず　スマホを縁の段差のところに置く
Dialogue: 0,0:17:22.06,0:17:27.06,DefaultSingle-J,,0,0,0,,		そして　これを使って　スマホから少し離れた床を濡らすと
Dialogue: 0,0:17:29.23,0:17:31.86,DefaultSingle-J,,0,0,0,,		ホントだ　スマホが浮いて見える
Dialogue: 0,0:17:32.44,0:17:33.57,DefaultSingle-J,,0,0,0,,		なるほど
Dialogue: 0,0:17:33.94,0:17:38.66,DefaultSingle-J,,0,0,0,,		酩酊状態の被害者がこれを見て　一瞬違和感を覚えたとしても
Dialogue: 0,0:17:38.91,0:17:43.79,DefaultSingle-J,,0,0,0,,		深く考えず　そのままその事実を受け入れてしまうかもしれませんね
Dialogue: 0,0:17:45.12,0:17:45.96,DefaultSingle-J,,0,0,0,,		じゃろ　
Dialogue: 0,0:17:46.37,0:17:51.34,DefaultSingle-J,,0,0,0,,		ただこの錯覚のキモは　スマホが宙に浮いて見えること自体ではなく
Dialogue: 0,0:17:51.67,0:17:55.38,DefaultSingle-J,,0,0,0,,		スマホが段差の手前にあるように見えることにあるんじゃ
Dialogue: 0,0:17:56.84,0:17:58.59,DefaultSingle-J,,0,0,0,,		どういうことですかな
Dialogue: 0,0:17:58.97,0:18:02.35,DefaultSingle-J,,0,0,0,,		つまり　着信音で目を覚ました浩平さんには
Dialogue: 0,0:18:02.72,0:18:05.01,DefaultSingle-J,,0,0,0,,		仕掛けられた錯覚のせいで　
Dialogue: 0,0:18:05.01,0:18:10.86,DefaultSingle-J,,0,0,0,,		段差にぶつかる前にスマホに手が届くという　目算が出来てしまったということじゃよ
Dialogue: 0,0:18:11.15,0:18:14.11,DefaultSingle-J,,0,0,0,,		じゃから　彼はさほど慎重にならずに
Dialogue: 0,0:18:14.11,0:18:16.49,DefaultSingle-J,,0,0,0,,		安心してスマホを拾いに向かった
Dialogue: 0,0:18:16.90,0:18:21.28,DefaultSingle-J,,0,0,0,,		じゃが　実際はスマホは段差の上　彼の目算は外れ
Dialogue: 0,0:18:21.66,0:18:25.87,DefaultSingle-J,,0,0,0,,		スマホに手が届く前に　段差につまずいてしまい
Dialogue: 0,0:18:28.29,0:18:31.50,DefaultSingle-J,,0,0,0,,		転倒して落下してしまったというわけじゃ
Dialogue: 0,0:18:32.92,0:18:36.63,DefaultSingle-J,,0,0,0,,		この錯覚は地面が乾けば消えてなくなるため
Dialogue: 0,0:18:36.63,0:18:39.55,DefaultSingle-J,,0,0,0,,		痕跡が残らんという利点もあるのじゃ
Dialogue: 0,0:18:39.93,0:18:40.89,DefaultSingle-J,,0,0,0,,		なるほど
Dialogue: 0,0:18:41.35,0:18:47.23,DefaultSingle-J,,0,0,0,,		手すりは事前に腐らせて　軽い弾みでも壊れるように準備しておったんじゃろうな
Dialogue: 0,0:18:47.98,0:18:50.73,DefaultSingle-J,,0,0,0,,		あの　証拠はあるんですか
Dialogue: 0,0:18:51.61,0:18:52.90,DefaultSingle-J,,0,0,0,,		もちろんじゃ
Dialogue: 0,0:18:53.36,0:18:55.61,DefaultSingle-J,,0,0,0,,		どんな証拠があるっていうんです
Dialogue: 0,0:18:56.15,0:19:01.29,DefaultSingle-J,,0,0,0,,		梶木さんはおそらくシミ隠しのために　顔にファンデを塗っておったじゃろ
Dialogue: 0,0:19:01.95,0:19:04.95,DefaultSingle-J,,0,0,0,,		じゃが　遺体からはコンタクトが外れているのに
Dialogue: 0,0:19:04.95,0:19:07.58,DefaultSingle-J,,0,0,0,,		目の周りをこすった跡がなかった
Dialogue: 0,0:19:08.00,0:19:13.71,DefaultSingle-J,,0,0,0,,		その代わり　上まぶたと下まぶたに指の跡らしきものがかすかに残っておった
Dialogue: 0,0:19:14.63,0:19:17.31,DefaultSingle-J,,0,0,0,,		確かに　そんな跡が残っていました
Dialogue: 0,0:19:17.76,0:19:22.64,DefaultSingle-J,,0,0,0,,		本人が自分で目を開いて　片目だけコンタクトを外すわけもなく
Dialogue: 0,0:19:22.89,0:19:28.27,DefaultSingle-J,,0,0,0,,		誰かが彼の目を開いて　コンタクトを取り外したと考えるのが自然じゃろ
Dialogue: 0,0:19:28.93,0:19:30.19,DefaultSingle-J,,0,0,0,,		そして　あんた　
Dialogue: 0,0:19:30.44,0:19:35.52,DefaultSingle-J,,0,0,0,,		さっき　元太君が水をこぼした時にハンカチを出そうとしてやめたじゃろ
Dialogue: 0,0:19:36.07,0:19:41.45,DefaultSingle-J,,0,0,0,,		そのハンカチには指についたファンデを拭いた跡が残ってるんじゃないかね
Dialogue: 0,0:19:42.41,0:19:44.53,DefaultSingle-J,,0,0,0,,		でも　何のためにそんなことを
Dialogue: 0,0:19:45.16,0:19:49.29,DefaultSingle-J,,0,0,0,,		両目の視力差が大きくなると遠近感が狂って
Dialogue: 0,0:19:49.29,0:19:51.16,DefaultSingle-J,,0,0,0,,		転倒しやすくなるんじゃよ
Dialogue: 0,0:19:51.58,0:19:53.54,DefaultSingle-J,,0,0,0,,		錯覚のトリックと合わせれば
Dialogue: 0,0:19:53.54,0:19:57.46,DefaultSingle-J,,0,0,0,,		浩平さんが段差で転ぶ確率がグンと上がるというわけじゃ
Dialogue: 0,0:19:58.09,0:20:02.38,DefaultSingle-J,,0,0,0,,		おそらく　小森さんが持っているハンカチには　ファンデの跡が
Dialogue: 0,0:20:02.72,0:20:08.14,DefaultSingle-J,,0,0,0,,		そして　この家のどこかに梶木さんのケータイを鳴らした電話があるはずじゃ
Dialogue: 0,0:20:08.93,0:20:12.36,DefaultSingle-J,,0,0,0,,		小森さん　何かおっしゃりたいことはありますかな
Dialogue: 0,0:20:12.73,0:20:13.48,DefaultSingle-J,,0,0,0,,		それと　
Dialogue: 0,0:20:13.85,0:20:18.07,DefaultSingle-J,,0,0,0,,		お持ちのハンカチと　家の中を調べさせてもらっても構いませんか
Dialogue: 0,0:20:20.74,0:20:22.36,DefaultSingle-J,,0,0,0,,		うまくいってたのに
Dialogue: 0,0:20:23.36,0:20:24.53,DefaultSingle-J,,0,0,0,,		朋子おばさん
Dialogue: 0,0:20:24.66,0:20:25.87,DefaultSingle-J,,0,0,0,,		近寄るんじゃないわよ
Dialogue: 0,0:20:26.24,0:20:28.37,DefaultSingle-J,,0,0,0,,		朋子さん　どうしちゃったんですか
Dialogue: 0,0:20:29.08,0:20:33.42,DefaultSingle-J,,0,0,0,,		ったく　せっかくの大芝居に泥を塗ってくれちゃって
Dialogue: 0,0:20:33.67,0:20:38.00,DefaultSingle-J,,0,0,0,,		あたしはね　あんた達みたいなガキ連中なんか大嫌いなんだよ
Dialogue: 0,0:20:38.84,0:20:39.92,DefaultSingle-J,,0,0,0,,		そうすると　
Dialogue: 0,0:20:40.21,0:20:42.88,DefaultSingle-J,,0,0,0,,		あんたははじめから子供達を利用して
Dialogue: 0,0:20:43.88,0:20:44.59,DefaultSingle-J,,0,0,0,,		そうよ
Dialogue: 0,0:20:44.89,0:20:48.72,DefaultSingle-J,,0,0,0,,		あのヤローが雑誌の取材で子供好き語ってやがったから
Dialogue: 0,0:20:49.97,0:20:54.81,DefaultSingle-J,,0,0,0,,		狙った大きな魚を釣るには　まず小魚を釣らないとなぁ
Dialogue: 0,0:20:54.81,0:20:56.77,DefaultSingle-J,,0,0,0,,		お　おい　大変だぞ
Dialogue: 0,0:20:57.06,0:20:59.15,DefaultSingle-J,,0,0,0,,		ここ　こ　この部屋に
Dialogue: 0,0:21:02.74,0:21:03.49,DefaultSingle-J,,0,0,0,,		これは
Dialogue: 0,0:21:04.95,0:21:05.66,DefaultSingle-J,,0,0,0,,		まさか
Dialogue: 0,0:21:06.82,0:21:08.87,DefaultSingle-J,,0,0,0,,		全員ってことはないですよね
Dialogue: 0,0:21:09.58,0:21:13.75,DefaultSingle-J,,0,0,0,,		ったく　せっかくの計画があんたらのせいで台なし
Dialogue: 0,0:21:14.04,0:21:15.25,DefaultSingle-J,,0,0,0,,		小森朋子さん
Dialogue: 0,0:21:15.58,0:21:18.29,DefaultSingle-J,,0,0,0,,		続きは署の方でじっくりお聞きします
Dialogue: 0,0:21:19.25,0:21:21.51,DefaultSingle-J,,0,0,0,,		話してもいいけどよぉ
Dialogue: 0,0:21:22.76,0:21:26.84,DefaultSingle-J,,0,0,0,,		お前達に理解できんのかよ
Dialogue: 0,0:22:54.06,0:22:56.02,DefaultSingle-J,,0,0,0,,		嫌な事件でしたね
Dialogue: 0,0:22:56.39,0:23:00.69,DefaultSingle-J,,0,0,0,,		どうやら彼女　同じような犯行をくり返していたみたいね
Dialogue: 0,0:23:01.23,0:23:03.57,DefaultSingle-J,,0,0,0,,		とんでもねえ犯罪者だったんだな
Dialogue: 0,0:23:03.82,0:23:06.69,DefaultSingle-J,,0,0,0,,		人は見かけによらぬものってとこかしら
Dialogue: 0,0:23:06.94,0:23:08.53,DefaultSingle-J,,0,0,0,,		ねえ　あの人
Dialogue: 0,0:23:09.28,0:23:10.57,DefaultSingle-J,,0,0,0,,		何してるんだろう
Dialogue: 0,0:23:12.12,0:23:14.41,DefaultSingle-J,,0,0,0,,		猫でも助けようとしてんじゃねえか
Dialogue: 0,0:23:14.78,0:23:16.75,DefaultSingle-J,,0,0,0,,		でも　猫がいる様子は
Dialogue: 0,0:23:17.41,0:23:17.95,DefaultSingle-J,,0,0,0,,		なあ　
Dialogue: 0,0:23:18.16,0:23:20.87,DefaultSingle-J,,0,0,0,,		何してるか　直接聞いてみようぜ
Dialogue: 0,0:23:21.33,0:23:22.33,DefaultSingle-J,,0,0,0,,		そうですね
Dialogue: 0,0:23:22.88,0:23:24.96,DefaultSingle-J,,0,0,0,,		女性が１人木に登って
Dialogue: 0,0:23:25.59,0:23:27.26,DefaultSingle-J,,0,0,0,,		絶対に訳アリですよ
Dialogue: 0,0:23:27.59,0:23:28.92,DefaultSingle-J,,0,0,0,,		やめとけって
Dialogue: 0,0:23:29.13,0:23:31.34,DefaultSingle-J,,0,0,0,,		でもよ　気になるじゃねえか
Dialogue: 0,0:23:31.34,0:23:32.26,DefaultSingle-J,,0,0,0,,		歩美も
Dialogue: 0,0:23:32.43,0:23:33.35,DefaultSingle-J,,0,0,0,,		行ってみましょう
Dialogue: 0,0:23:33.51,0:23:33.89,DefaultSingle-J,,0,0,0,,		うん
Dialogue: 0,0:23:34.64,0:23:35.47,DefaultSingle-J,,0,0,0,,		おい
Dialogue: 0,0:23:40.56,0:23:41.69,DefaultSingle-J,,0,0,0,,		行っちゃいましたね
Dialogue: 0,0:23:42.02,0:23:42.65,DefaultSingle-J,,0,0,0,,		ね
Dialogue: 0,0:23:46.52,0:23:47.70,DefaultSingle-J,,0,0,0,,		すごい
Dialogue: 0,0:23:48.15,0:23:51.20,DefaultSingle-J,,0,0,0,,		これキャンプじゃなくて　グランピングっていうだよね
Dialogue: 0,0:23:51.86,0:23:55.24,DefaultSingle-J,,0,0,0,,		女装した上に　顔にはへたっぴな落書き
Dialogue: 0,0:23:55.53,0:23:59.21,DefaultSingle-J,,0,0,0,,		口はカニの足をくわえさせ　手にはメモ
Dialogue: 0,0:23:59.58,0:24:01.09,DefaultSingle-J,,0,0,0,,		私　こんなことしてない
Dialogue: 0,0:24:02.62,0:24:03.96,DefaultSingle-J,,0,0,0,,		なんか増えてやがる
Dialogue: 0,0:24:06.09,0:24:08.80,DefaultSingle-J,,0,0,0,,		よし　これで準備は完了だ
Dialogue: 0,0:24:09.67,0:24:10.84,DefaultSingle-J,,0,0,0,,		Next　Conan
Dialogue: 0,0:24:11.17,0:24:13.22,DefaultSingle-J,,0,0,0,,		グランピング怪事件
Dialogue: 0,0:24:30.57,0:24:32.24,DefaultSingle-J,,0,0,0,,		Next　Conan's　hint
Dialogue: 0,0:24:32.74,0:24:33.78,DefaultSingle-J,,0,0,0,,		カニ
Dialogue: 0,0:24:34.95,0:24:36.12,DefaultSingle-J,,0,0,0,,	{\bord4}	グランピングって
Dialogue: 0,0:24:36.12,0:24:36.91,DefaultSingle-J,,0,0,0,,	{\bord4}	リッチなキャンプ
Dialogue: 0,0:24:36.91,0:24:37.91,DefaultSingle-J,,0,0,0,,	{\bord4}	行きたい
Dialogue: 0,0:24:37.91,0:24:39.33,DefaultSingle-J,,0,0,0,,	{\bord4}	絶対何か起きるから　ダメ
Dialogue: 0,0:00:00.00,0:00:05.00,DefaultSingle-J,,0,0,0,,
